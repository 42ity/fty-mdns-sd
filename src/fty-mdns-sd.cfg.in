#   fty-mdns-sd configuration
server
    verbose = true                                 #   To setup verbose

fty-info
    name = fty-info
    command = INFO

malamute
    endpoint = ipc://@/malamute                     #   Malamute endpoint
    address = fty-mdns-sd                           #   Agent mdns-sd address=

scan
    daemonactive = false
    stdout = false
    nobusout = false
    command = START-SCAN
    topic = SCAN-ANNOUNCE
    type = _https._tcp

log
    config = /etc/fty/ftylog.cfg
